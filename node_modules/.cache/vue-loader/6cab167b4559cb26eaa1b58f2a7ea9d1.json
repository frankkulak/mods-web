d-5 col-lg-4\"\n                 v-for=\"(mod, index) in mods\"\n                 :key=\"index\">\n                <!--                <mod-preview :mod=\"mod\"></mod-preview>-->\n                <p>{{ mod.id }}</p>\n            </div>\n        </section>\n\n        <section id=\"contact\" class=\"row justify-content-center\">\n            <div class=\"col-12 col-md-10 col-lg-8\">\n                <h1>contact</h1>\n                <p>email: <a href=\"mailto:mods@frankkulak.com\">mods@frankkulak.com</a></p>\n                <p>Feel free to report issues or ask questions, but please refrain from sending mod requests. I\n                    am still new to modding and am in the process of figuring it out.</p>\n            </div>\n        </section>\n    </div>\n</template>\n\n<script>\n    import Data from '../data/data.js'\n\n    export default {\n        name: \"HomePage\",\n        data: function () {\n            return {\n                mods: Data.ts4\n            }\n        }\n    }\n</script>\n\n<style scoped lang=\"scss\">\n    $padding-lg: 40px;\n    $padding-md: 20px;\n    $padding-sm: 10px;\n    $padding-xs: 5px;\n\n    h1, p {\n        margin-bottom: $padding-md;\n\n        &:last-child {\n            margin-bottom: 0;\n        }\n    }\n\n    #welcome {\n        padding-top: $padding-lg;\n        padding-bottom: 0;\n    }\n\n    #mods {\n        padding-top: $padding-lg - $padding-sm;\n        padding-bottom: $padding-lg - $padding-sm;\n\n        [class^=\"col\"] {\n            margin: {\n                top: $padding-sm;\n                bottom: $padding-sm;\n            }\n        }\n    }\n\n    #contact {\n        background-color: var(--bg-band-overlay);\n        padding-top: $padding-lg;\n        padding-bottom: $padding-lg;\n    }\n</style>"]}]}