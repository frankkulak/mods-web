{$blue};\n        --button-color: #{$blue};\n        --button-text-color: #{$off-white};\n        --hr-color: #{transparentize($blue, 0.5)};\n        --bg-band-overlay: #{transparentize(black, 0.95)};\n        --text-highlight-color: #{transparentize($blue, 0.85)};\n        --shadow-color: #{transparentize($dark-gray, 0.85)};\n        --success-color: #{$green};\n        --warning-color: #{$yellow};\n        --danger-color: #{$red};\n        --light-color: #{$off-white};\n        --dark-color: #{$dark-gray};\n    }\n\n    @mixin gradient($bottom, $top, $fallback) {\n        background-color: $fallback;\n        background-image: linear-gradient(45deg, $bottom 0%, $top 100%);\n    }\n\n    %default-gradient {\n        @include gradient($bottom: $blue, $top: $teal, $fallback: $blue);\n    }\n\n    // base ------------------------------------------------------------------------------------------------------------\n\n    body {\n        background-color: var(--bg-color);\n        position: relative;\n        margin: 0;\n        padding: 0;\n    }\n\n    p, h1, h2, h3, h4, h5, h6 {\n        padding: 0;\n        margin: 0;\n    }\n\n    hr {\n        background-color: var(--hr-color);\n        height: 2px;\n        border-radius: 1px;\n        border: 0;\n    }\n\n    a.btn, a.btn-outline-primary, a.btn-outline-primary:focus {\n        color: var(--button-color);\n        border-color: var(--button-color);\n        text-decoration: none;\n    }\n\n    a.btn-outline-primary:hover {\n        @extend %default-gradient;\n        color: var(--button-text-color);\n        border-style: none;\n        text-decoration: none;\n    }\n\n    .status-success {\n        background-color: var(--success-color);\n        color: var(--light-color);\n    }\n\n    .status-warning {\n        background-color: var(--warning-color);\n        color: var(--dark-color);\n    }\n\n    .status-danger {\n        background-color: var(--danger-color);\n        color: var(--light-color);\n    }\n\n    span.unbold {\n        font-weight: normal;\n    }\n\n    span.important {\n        font-weight: bold;\n        color: var(--danger-color);\n    }\n\n    p.disclaimer {\n        font-size: 14px;\n    }\n\n    // typography ------------------------------------------------------------------------------------------------------\n\n    body {\n        font-family: 'Muli', sans-serif;\n        font-weight: 300;\n        font-size: 18px;\n        color: var(--text-color);\n    }\n\n    a, a:focus {\n        color: var(--link-color);\n        text-decoration: none;\n    }\n\n    a:hover {\n        color: var(--link-hover-color);\n        text-decoration: underline;\n    }\n\n    h1, h2, h3, h4, h5, h6 {\n        color: var(--h-color);\n        font-weight: bold;\n    }\n\n    h1 {\n        font-size: 28px;\n    }\n\n    h2 {\n        font-size: 26px;\n    }\n\n    h3 {\n        font-size: 24px;\n    }\n\n    h4 {\n        font-size: 22px;\n    }\n\n    h5 {\n        font-size: 20px;\n    }\n\n    h6 {\n        font-size: 18px;\n    }\n\n</style>"]}]}